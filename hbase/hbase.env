HBASE_CONF_hbase_rootdir=hdfs://namenode:9000/hbase
HBASE_CONF_hbase_cluster_distributed=true
HBASE_CONF_hbase_zookeeper_quorum=hbase-master,hbase-regionserver

HBASE_CONF_hbase_master=hbase-master:16000
HBASE_CONF_hbase_master_hostname=hbase-master
HBASE_CONF_hbase_master_port=16000
HBASE_CONF_hbase_master_info_port=16010
HBASE_CONF_hbase_regionserver_port=16020
HBASE_CONF_hbase_regionserver_info_port=16030

HBASE_CONF_hbase_tmp_dir=/opt/hbase-2.3.5/tmp

HBASE_MANAGES_ZK=false

HBASE_CONF_hbase_coprocessor_regionserver_classes=umc.cdc.hds.coprocessor.HdsRegionServerObserver
HDS_CONF_hds_config_location=/opt/hdrs-1.1.0-without-cdh/conf
HDS_CONF_hds_httpserver_port=8000
HDS_CONF_hds_logger_enable=false
HDS_CONF_hds_httpserver_task_long_num=20
HDS_CONF_hds_httpserver_task_short_num=20
HDS_CONF_hds_memory_buffer_upper_limit=209715200
HDS_CONF_hds_logger_jdbc_url=jdbc:phoenix:hbase-regionserver:2181
HDS_CONF_hds_logger_jdbc_driver=org.apache.phoenix.jdbc.PhoenixDriver
HDS_CONF_hds_logger_jdbc_db=phoenix
HDS_CONF_hds_logger_queue_size=100000
HDS_CONF_hds_logger_batch_size=3000
HDS_CONF_hds_httpserver_cors_allow=true
HDS_CONF_hds_httpserver_cors_origins=*

ZOO_CONF_DIR=/zookeeper/conf
ZOO_DATA_DIR=/zookeeper/data
ZOO_DATA_LOG_DIR=/zookeeper/datalog
ZOO_LOG_DIR=/zookeeper/logs
ZOO_TICK_TIME=2000
ZOO_INIT_LIMIT=5
ZOO_SYNC_LIMIT=2
ZOO_AUTOPURGE_PURGEINTERVAL=0
ZOO_AUTOPURGE_SNAPRETAINCOUNT=3
ZOO_MAX_CLIENT_CNXNS=60
ZOO_STANDALONE_ENABLED=true
ZOO_ADMINSERVER_ENABLED=true